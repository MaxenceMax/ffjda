<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:background="@color/white_transparent">
    <LinearLayout
        android:layout_centerVertical="true"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_marginLeft="15dp"
        android:layout_marginTop="10dp"
        android:layout_marginBottom="10dp">
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="JUDO elf krj krvj zkj ekjze fkzejf ez"
            android:textSize="20dp"
            android:textStyle="bold"
            android:textColor="@color/blue"
            android:id="@+id/historique_club_row_club"/>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical">
                <LinearLayout
                    android:layout_marginTop="5dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">
                    <TextView
                        android:textColor="@color/blue"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/Arrivee_"/>
                    <TextView
                        android:id="@+id/historique_club_row_saison_arrivee"
                        android:layout_marginLeft="10dp"
                        android:textColor="@color/blue"
                        android:textStyle="bold"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="2012-2013"/>
                </LinearLayout>
                <LinearLayout
                    android:layout_marginTop="5dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">
                    <TextView
                        android:textColor="@color/blue"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/Depart_"/>
                    <TextView
                        android:id="@+id/historique_club_row_saison_depart"
                        android:layout_marginLeft="10dp"
                        android:textColor="@color/blue"
                        android:textStyle="bold"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="2012-2013"/>
                </LinearLayout>
            </LinearLayout>
            <TextView
                android:layout_marginTop="-10dp"
                android:layout_marginLeft="90dp"
                android:id="@+id/historique_club_row_years"
                android:layout_marginRight="5dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="55dp"
                android:textColor="@color/blue"
                android:text="2"/>
            <TextView
                android:layout_marginTop="-10dp"
                android:id="@+id/historique_club_row_year_label"
                android:layout_marginRight="10dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/blue"
                android:textSize="15dp"
                android:text="an"/>
            </LinearLayout>
        </LinearLayout>
</RelativeLayout>
